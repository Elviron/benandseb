<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background" android:drawable="@drawable/seekbar_progress_bg">
        
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip
            android:clipOrientation="horizontal"
            android:gravity="left" >
            <shape>
                <gradient
                    android:angle="0"
                    android:centerColor="@color/EL_icon_bolt"
                    android:centerX="0.75"
                    android:endColor="@color/EL_icon_bolt"
                    android:startColor="@color/EL_icon_bolt" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip
            android:clipOrientation="horizontal"
            android:gravity="left" android:drawable="@drawable/seekbar_progress_primary" >
        </clip>
    </item>
</layer-list>